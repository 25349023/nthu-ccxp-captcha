{
  "name": "NTHU CCXP Decaptcha",
  "description": "Decaptcha for NTHU Academic Information Systems",
  "version": "0.3.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://www.ccxp.nthu.edu.tw/ccxp/INQUIRE/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "icons": {
    "32": "/icons/ccxp_decaptcha@32.png",
    "48": "/icons/ccxp_decaptcha@48.png",
    "128": "/icons/ccxp_decaptcha@128.png"
  },
  "permissions": [
    "*://7s7izxcb32.execute-api.us-east-1.amazonaws.com/*",
    "*://www.ccxp.nthu.edu.tw/ccxp/INQUIRE/*"
  ]
}